<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sidhupvttechltd.www.inventoryapp2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sidhupvttechltd.www.inventoryapp2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sidhupvttechltd.www.inventoryapp2.EditorActivity"
            android:parentActivityName="com.sidhupvttechltd.www.inventoryapp2.MainActivity"
            />
        <provider
            android:authorities="com.sidhupvttechltd.www.inventoryapp2"
            android:exported="false"
            android:name="com.sidhupvttechltd.www.inventoryapp2.data.ProductProvider"/>
    </application>

</manifest>